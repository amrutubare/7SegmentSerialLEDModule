config BOARD_NUCLEO_L073RZ_CUSTOM
	bool "NUCLEO-L073RZ_CUSTOM"
	depends on CHIP_STM32L0
	select CHIP_STM32L073RZ
	select CHIP_LQFP64
	help
		NUCLEO-L073RZ customized board with STM32L073RZ chip in LQFP64 package
